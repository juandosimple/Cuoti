{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "sql:default",
    {
      "identifier": "sql:allow-execute",
      "allow": [
        {
          "name": "sqlite:cuoti.db",
          "sql": "*"
        }
      ]
    },
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:11434/*"
        }
      ]
    },
    "shell:allow-open"
  ]
}